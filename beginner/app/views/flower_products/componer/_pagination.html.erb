<div class="row">
    <div class="col-xs-12 col-md-12">
        <nav aria-label="Page navigation">
            <ul class="pagination">
                <%if page >1 %>
                <li>
                    <%=link_to send(url_coustom_helor_name,{page:(page-1)}) do%>
                    <span aria-hidden="true">&laquo;</span>
                </li>
                <%end%>
                <%end%>

                <% (first_page..last_page).each do |page_number|%>
                <li><%=link_to page_number,send(url_coustom_helor_name,{page: page_number})%></a></li>
                <%end%>



                <%if page < @last_page%>
                <li>
                    <%=link_to send(url_coustom_helor_name,{page:(page+1)}) do%>
                    <span aria-hidden="true">&raquo;</span>
                    <%end%>
                </li>
                <%end%>

            </ul>


        </nav>
    </div>
</div>